<% content_for :page_title, "Sign up" %>

<h2>Sign up</h2>

<%= simple_form_for @user do |f| %>

  <%= f.input :username %>
  <%= f.input :email %>
  <%= f.input :studio_number, label: "Studio number" %>
  <!-- <%= f.input :phone_number, label: "Whatsapp" %> -->
  <%= f.input :password %>
  <%= f.input :password_confirmation %>

  <%= f.button :submit, "Sign up" %>

<% end %>
